{
  "name": "f3-region-pages",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.18.2"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npm run db:migrate && npm run db:seed && next build",
    "next:build": "next build",
    "start": "next start",
    "start:with-migrations": "npm run db:migrate && npm run db:seed && next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate:migration": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "db:push": "drizzle-kit push --config drizzle.config.ts",
    "db:reset": "bun scripts/reset.ts",
    "db:seed": "bun scripts/seed.ts",
    "docker:kill": "scripts/docker-kill.sh",
    "supabase:start": "supabase start",
    "deploy:app": "scripts/deploy-app.sh",
    "deploy:db": "scripts/deploy-db.sh",
    "deploy": "npm run deploy:db && npm run deploy:app"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/pg": "^8.11.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "bun": "^1.2.4",
    "drizzle-kit": "^0.30.4",
    "drizzle-orm": "^0.39.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "jest": "^29.7.0",
    "pg": "^8.13.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5"
  }
}
